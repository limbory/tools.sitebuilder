    "tool": "better-npm-run tool",
    "ghpages:dev": "better-npm-run ghpages:debug",
    "ghpages:production": "better-npm-run ghpages:build"
  },
  "betterScripts": {
    "tool": {
      "command": "gulp",
      "env": {
        "PROJECT": "custom.tools"
      }
